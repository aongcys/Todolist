{
  "name": "@app/db",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && pnpm generate && tsc --build",
    "push": "prisma db push",
    "seed": "tsx ./prisma/seeds/index.ts",
    "studio": "prisma studio --schema ./prisma/schema.prisma",
    "generate": "prisma generate",
    "migrate:dev": "npx prisma migrate dev --schema ./prisma/schema.prisma",
    "migrate:draft": "npx prisma migrate diff --schema ./prisma/schema.prisma",
    "migrate:deploy": "npx prisma migrate deploy --schema ./prisma/schema.prisma",
    "migrate:reset": "npx prisma migrate reset --schema ./prisma/schema.prisma",
    "prisma": "prisma"
  },
  "dependencies": {
    "@nestjs/common": "11.1.6",
    "@prisma/client": "6.16.2"
  },
  "devDependencies": {
    "prisma": "6.16.2"
  }
}